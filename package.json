{
  "name": "lil-wingman",
  "version": "0.2.0",
  "license": "MIT",
  "author": "lucystwt",
  "description": "A JavaScript helper library, written in TypeScript.",
  "keywords": [
    "lib",
    "library",
    "helpers",
    "utils"
  ],
  "homepage": "https://github.com/lucystwt/wingman",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucystwt/wingman"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:types && rollup -c",
    "build:types": "tsc -b ./tsconfig.types.json",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
    "ci": "pnpm build && pnpm test && pnpm lint && pnpm check",
    "prepublishOnly": "pnpm ci"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1",
    "rollup-plugin-dts": "^5.1.1",
    "ts-jest": "^29.0.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}